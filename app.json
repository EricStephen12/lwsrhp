{
  "expo": {
    "name": "lwsrhp",
    "slug": "lwsrhp",
    "scheme": "lwsrhp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-fixed.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon-fixed.png",
      "resizeMode": "contain",
      "backgroundColor": "#800080"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.loveworld.lwsrhp",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera for video calls.",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone for voice and video calls.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to share images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-fixed.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/icon.png",
      "package": "com.loveworld.lwsrhp",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "WAKE_LOCK",
        "VIBRATE",
        "READ_SYNC_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "blockedPermissions": [],
      "softwareKeyboardLayoutMode": "resize",
      "statusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#ffffff",
        "translucent": false
      },
      "navigationBar": {
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon-fixed.png",
          "color": "#800080",
          "sounds": []
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone for voice and video calls."
        }
      ],
      "./plugins/withSecureFlag.js",
      "expo-web-browser"
    ],
    "notification": {
      "icon": "./assets/icon-fixed.png",
      "color": "#800080",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new notifications"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e002cb6d-69f0-4035-8f09-357365fcd7a5"
      }
    },
    "owner": "lwsrhpapp"
  }
}
